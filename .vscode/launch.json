    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch Server",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/out/dedicated_launcher.exe",
            "args": ["-game", "chungus", "-console"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/build/out",
            "environment": [],
            "console": "internalConsole"
        },
        {
            "name": "(Windows) Launch Game",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/out/hl2_launcher.exe",
            "args": ["-game", "hl2", "-windowed", "-width", "1200", "-height", "768", "-dxlevel", "95"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/build/out",
            "environment": [],
            "console": "internalConsole"
        }
    ],
    
}